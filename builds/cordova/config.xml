<?xml version='1.0' encoding='utf-8'?>
<widget id="com.criticalthinking.funtimephonics" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fun-Time Phonics</name>
    <description>
        The Simplest, Most Effective Way to Learn to Read!
    </description>
    <author email="jimmym@criticalthinking.com" href="http://www.criticalthinking.com/">
        The Critical Thinking Co.
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="Orientation" value="landscape" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orienation" value="landscape" />
    <engine name="ios" spec="~4.1.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" spec="https://github.com/katzer/cordova-plugin-hidden-statusbar-overlay.git" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.4.0" />
    <plugin name="ionic-plugin-keyboard" spec="https://github.com/driftyco/ionic-plugins-keyboard.git" />
</widget>
