    "create-asset-index": "node scripts/create-asset-index.js",
    "dev-server": "npm run create-asset-index && scripts/dev-server",
    "xcode": "open ./builds/cordova/platforms/ios/ftph.xcodeproj",
    "upload": "scripts/upload"
  },
  "dependencies": {
    "babel-core": "^6.3.21",
