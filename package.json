    "howler": "^1.1.26",
    "lodash": "^3.10.1",
    "phaser": "^2.4.3",
    "pixi.js": "^3.0.7",
    "putainde-localstorage": "^4.0.0",
    "react": "^0.14.0",
    "react-addons-create-fragment": "^0.14.2",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dnd-touch-backend": "^0.2.0",
    "react-dom": "^0.14.1",
    "rx": "^3.1.2"
  },
