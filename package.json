{
  "name": "fun-time-refactored",
  "version": "0.0.11",
  "scripts": {
    "clean": "mkdir -p builds && mkdir -p dist && rm -rf builds/* && rm -rf dist/*",
    "setup": "npm run setup-cordova && npm run convert-audio",
    "setup-cordova": "scripts/setup-cordova",
    "build-cordova": "scripts/compile && scripts/build-cordova",
    "convert-audio": "node scripts/convert-audio.js",
    "create-sprites": "node scripts/create-sprites",
    "compile": "scripts/compile",
    "start": "scripts/dev-server",
    "upload": "scripts/upload",
    "release": "npm run compile && npm run upload",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --recursive --require babel-polyfill --compilers scss:./null-compiler.js,js:babel-core/register 'src/**/*.test.js'",
    "convert-word-images": "scripts/convert-word-images"
  },
  "dependencies": {
    "bembam": "^1.0.3",
    "color": "^0.11.1",
    "howler": "^1.1.26",
    "lodash": "^4.0.0",
